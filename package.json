{
  "name": "@datadayrepos/fast-average-color",
  "description": "Fork of original for supply chain security reasons. A simple library that calculates the average color of images, videos and canvas in browser environment.",
  "version": "9.4.0",
  "author": {
    "name": "Denis Seleznev (original author)",
    "email": "hcodes@yandex.ru",
    "url": "https://github.com/fast-average-color/fast-average-color"
  },
  "contributors": [
    {
      "name": "Datadayrepos Team",
      "url": "https://github.com/datadayrepos"
    }
  ],

  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.browser.js",
  "jsdelivr": "dist/index.browser.js",
  "homepage": "https://github.com/datadayrepos/fast-average-color",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/datadayrepos/fast-average-color.git"
  },
  "keywords": [
    "fast",
    "average",
    "color",
    "colour"
  ],
  "engines": {
    "node": ">= 12"
  },
  "typings": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@rollup/plugin-typescript": "^12.1.0",
    "@types/jest": "^29.5.13",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "del-cli": "^5.1.0",
    "eslint": "^9.11.1",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "rollup": "^4.22.4",
    "terser": "^5.33.0",
    "ts-jest": "^29.2.5",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "scripts": {
    "build": "npm run clean && npm run rollup && npm run minify && npm run copyright",
    "clean": "del dist/*",
    "minify": "terser ./dist/index.browser.js -o ./dist/index.browser.min.js -c -m",
    "copyright": "node tools/copyright.mjs dist/index.*js",
    "rollup": "rollup --config rollup.config.mjs",
    "test": "npm run typecheck && npm run eslint && npm run unit-test",
    "eslint": "eslint .",
    "unit-test": "jest .",
    "prepare": "npm run build",
    "typecheck": "tsc --noEmit"
  }
}
